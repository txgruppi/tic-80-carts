import "random" for Random
import "./lib/global"
import "./lib/menustate"
import "./lib/playstate"
import "./lib/gameoverstate"

class Game is TIC {
	construct new() {
		_state = MenuState.new()
		Global.resetWins()
	}

	state { _state }
	state=(v) { _state = v }

	TIC() {
		_state.TIC(this)
	}
}

